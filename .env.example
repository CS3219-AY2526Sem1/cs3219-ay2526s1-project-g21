TUNING_LEARNING_RATE=0.001
TUNING_EPOCH_COUNT=5
TUNING_ADAPTER_SIZE=ADAPTER_SIZE_ONE
TUNING_GCS_BUCKET=gs://your-bucket-name

CLIENT_BASE_URL=http://localhost:5173
USER_SERVICE_BASE_URL=http://user-service:8081

